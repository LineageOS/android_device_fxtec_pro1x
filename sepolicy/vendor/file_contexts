# Fingerprint
/dev/madev0                                                                u:object_r:fpsensor_fp_device:s0

# Hardware keyboard variants
/vendor/etc/keyboard(/.*)?                                                 u:object_r:vendor_keylayout_file:s0

# KeyDisabler
/(vendor|system/vendor)/bin/hw/vendor\.lineage\.touch\@[0-9]\.[0-9]-service.pro1    u:object_r:hal_lineage_touch_default_exec:s0

# Lights
/vendor/bin/hw/android\.hardware\.lights-service\.pro1x                    u:object_r:hal_light_default_exec:s0

# SSR
/sys/devices/platform/soc/5ab0000\.qcom,venus/subsys[0-9]+/name            u:object_r:vendor_sysfs_ssr:s0
/sys/devices/platform/soc/5ab0000\.qcom,venus/subsys[0-9]+/restart_level   u:object_r:vendor_sysfs_ssr_toggle:s0

# Sysfs (Wakeup)
/sys/devices/virtual/diag/diag/wakeup[0-9]+(/.*)?                          u:object_r:sysfs_wakeup:s0
/sys/devices/virtual/misc/msm_aac/wakeup[0-9]+(/.*)?                       u:object_r:sysfs_wakeup:s0
