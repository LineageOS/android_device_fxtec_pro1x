#
# F(x)tec Scandic keyboard layout
#

type OVERLAY

map key 86 PLUS

### ROW 1

key 1 {
    label:                              '1'
    base:                               '1'
    shift, fn:                          '!'
}

key 2 {
    label:                              '2'
    base:                               '2'
    shift, fn:                          '"'
    ralt:                               '@'
}

key 3 {
    label:                              '3'
    base:                               '3'
    shift, fn:                          '#'
    ralt:                               '\u00a3'
}

key 4 {
    label:                              '4'
    base:                               '4'
    shift, fn:                          '\u20ac'
    ralt:                               '$'
}

key 5 {
    label:                              '5'
    base:                               '5'
    shift, fn:                          '%'
    ralt:                               '\u20ac'
}

key 6 {
    label:                              '6'
    base:                               '6'
    shift, fn:                          '&'
}

key 7 {
    label:                              '7'
    base:                               '7'
    shift, fn:                          '/'
    ralt:                               '{'
}

key 8 {
    label:                              '8'
    base:                               '8'
    shift, fn:                          '('
    ralt:                               '['
}

key 9 {
    label:                              '9'
    base:                               '9'
    shift, fn:                          ')'
    ralt:                               ']'
}

key 0 {
    label:                              '0'
    base:                               '0'
    shift, fn:                          '='
    ralt:                               '}'
}

key MINUS {
    label:                              '+'
    base:                               '+'
    shift, fn:                          '?'
    ralt:                               '\\'
}

key EQUALS {
    label:                              '\u00b4'
    base:                               '\u0301'
    shift, fn:                          '\u0300'
}

### ROW 2

key Q {
    label:                              'Q'
    base:                               'q'
    shift, capslock:                    'Q'
    shift+capslock:                     'q'
    ralt:                               '\u00e2'
    ralt+capslock, shift+ralt:          '\u00c2'
    fn:                                 '\u00a7'
}

key W {
    label:                              'W'
    base:                               'w'
    shift, capslock:                    'W'
    shift+capslock:                     'w'
    fn:                                 '@'
}

key E {
    label:                              'E'
    base:                               'e'
    shift, capslock:                    'E'
    shift+capslock:                     'e'
    ralt, ralt+capslock:                '\u20ac'
    fn:                                 '$'
}

key R {
    label:                              'R'
    base:                               'r'
    shift, capslock:                    'R'
    shift+capslock:                     'r'
    fn:                                 '\u030a'
}

key T {
    label:                              'T'
    base:                               't'
    shift, capslock:                    'T'
    shift+capslock:                     't'
    ralt:                               '\u0167'
    ralt+capslock, shift+ralt:          '\u0166'
}

key Y {
    label:                              'Y'
    base:                               'y'
    shift, capslock:                    'Y'
    shift+capslock:                     'y'
}

key U {
    label:                              'U'
    base:                               'u'
    shift, capslock:                    'U'
    shift+capslock:                     'u'
    fn:                                 '{'
}

key I {
    label:                              'I'
    base:                               'i'
    shift, capslock:                    'I'
    shift+capslock:                     'i'
    ralt:                               '\u00ef'
    ralt+capslock, shift+ralt:          '\u00cf'
    fn:                                 '['
}

key O {
    label:                              'O'
    base:                               'o'
    shift, capslock:                    'O'
    shift+capslock:                     'o'
    ralt:                               '\u00f5'
    ralt+capslock, shift+ralt:          '\u00d5'
    fn:                                 ']'
}

key P {
    label:                              'P'
    base:                               'p'
    shift, capslock:                    'P'
    shift+capslock:                     'p'
    fn:                                 '}'
}

key LEFT_BRACKET {
    label:                              '\u00c5'
    base:                               '\u00e5'
    shift, capslock:                    '\u00c5'
    shift+capslock:                     '\u00e5'
    fn:                                 '\\'
}

key RIGHT_BRACKET {
    label:                              '\u00a8'
    base:                               '\u0308'
    shift, fn:                          '\u0302'
    ralt:                               '\u0303'
}

### ROW 3

key A {
    label:                              'A'
    base:                               'a'
    shift, capslock:                    'A'
    shift+capslock:                     'a'
    ralt:                               '\u00e1'
    ralt+capslock, shift+ralt:          '\u00c1'
}

key S {
    label:                              'S'
    base:                               's'
    shift, capslock:                    'S'
    shift+capslock:                     's'
    ralt:                               '\u0161'
    ralt+capslock, shift+ralt:          '\u0160'
}

key D {
    label:                              'D'
    base:                               'd'
    shift, capslock:                    'D'
    shift+capslock:                     'd'
    ralt:                               '\u0111'
    ralt+capslock, shift+ralt:          '\u0110'
}

key F {
    label:                              'F'
    base:                               'f'
    shift, capslock:                    'F'
    shift+capslock:                     'f'
    ralt:                               '\u01e5'
    ralt+capslock, shift+ralt:          '\u01e4'
}

key G {
    label:                              'G'
    base:                               'g'
    shift, capslock:                    'G'
    shift+capslock:                     'g'
    ralt:                               '\u01e7'
    ralt+capslock, shift+ralt:          '\u01e6'
}

key H {
    label:                              'H'
    base:                               'h'
    shift, capslock:                    'H'
    shift+capslock:                     'h'
    ralt:                               '\u021f'
    ralt+capslock, shift+ralt:          '\u021e'
}

key J {
    label:                              'J'
    base:                               'j'
    shift, capslock:                    'J'
    shift+capslock:                     'j'
}

key K {
    label:                              'K'
    base:                               'k'
    shift, capslock:                    'K'
    shift+capslock:                     'k'
    ralt:                               '\u01e9'
    ralt+capslock, shift+ralt:          '\u01e8'
    fn:                                 '*'
}

key L {
    label:                              'L'
    base:                               'l'
    shift, capslock:                    'L'
    shift+capslock:                     'l'
    fn:                                 '\''
}

key SEMICOLON {
    label:                              '\u00d6'
    base:                               '\u00f6'
    shift, capslock:                    '\u00d6'
    shift+capslock:                     '\u00f6'
    ralt, fn:                           '\u00f8'
    ralt+capslock, shift+ralt:          '\u00d8'
    fn+capslock, shift+fn:              '\u00d8'
}

key APOSTROPHE {
    label:                              '\u00c4'
    base:                               '\u00e4'
    shift, capslock:                    '\u00c4'
    shift+capslock:                     '\u00e4'
    ralt, fn:                           '\u00e6'
    ralt+capslock, shift+ralt:          '\u00c6'
    fn+capslock, shift+fn:              '\u00c6'
}

### ROW 4

key PLUS {
    label:                              '<'
    base:                               '<'
    shift, fn:                          '>'
    ralt:                               '|'
}

key Z {
    label:                              'Z'
    base:                               'z'
    shift, capslock:                    'Z'
    shift+capslock:                     'z'
    ralt:                               '\u017e'
    ralt+capslock, shift+ralt:          '\u017d'
    fn:					'|'
}

key X {
    label:                              'X'
    base:                               'x'
    shift, capslock:                    'X'
    shift+capslock:                     'x'
}

key C {
    label:                              'C'
    base:                               'c'
    shift, capslock:                    'C'
    shift+capslock:                     'c'
    ralt:                               '\u010d'
    ralt+capslock, shift+ralt:          '\u010c'
}

key V {
    label:                              'V'
    base:                               'v'
    shift, capslock:                    'V'
    shift+capslock:                     'v'
    ralt:                               '\u01ef'
    ralt+capslock, shift+ralt:          '\u01ee'
}

key B {
    label:                              'B'
    base:                               'b'
    shift, capslock:                    'B'
    shift+capslock:                     'b'
    ralt:                               '\u0292'
    ralt+capslock, shift+ralt:          '\u01b7'
}

key N {
    label:                              'N'
    base:                               'n'
    shift, capslock:                    'N'
    shift+capslock:                     'n'
    ralt:                               '\u014b'
    ralt+capslock, shift+ralt:          '\u014a'
    fn:					'\u0303'
}

key M {
    label:                              'M'
    base:                               'm'
    shift, capslock:                    'M'
    shift+capslock:                     'm'
    ralt, ralt+capslock:                '\u00b5'
}

key COMMA {
    label:                              ','
    base:                               ','
    shift, fn:                          ';'
}

key PERIOD {
    label:                              '.'
    base:                               '.'
    shift, fn:                          ':'
}

key SLASH {
    label:                              '-'
    base:                               '-'
    shift, fn:                          '_'
}
